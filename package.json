{
  "name": "mon-projet-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "npx prettier --write .",
    "push-model": "npx prisma db push",
    "postinstall": "prisma generate",
    "test": "jest",
    "test-clear-cache": "jest --clearCache",
    "insert-data-in-db": "node scripts/insert-data-in-db.mjs",
    "extract-data": "node scripts/extract-data.mjs",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down",
    "dc:dev": "docker-compose exec app npm run dev",
    "prisma:gen": "docker-compose exec app npx prisma generate",
    "prisma:push": "docker-compose exec app npx prisma db push",
    "db:import": "docker-compose exec mongo mongoimport --db engine_data --collection Article --type csv --headerline --file /scripts/articles.csv",
    "prisma:studio": "docker-compose exec app npx prisma studio --hostname 0.0.0.0 --port 5555"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "csv-writer": "^1.6.0",
    "jsdom": "^22.1.0",
    "lucide-react": "^0.292.0",
    "next": "14.0.2",
    "node-fetch": "^2.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.5.2",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/cheerio": "^0.22.34",
    "@types/jsdom": "^21.1.5",
    "@types/json2csv": "^5.0.6",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.5.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.2",
    "i": "^0.3.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "npm": "^10.2.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
